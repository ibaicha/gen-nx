{
  "name": "angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/angular/src",
  "tags": [],
  "targets": {
    "prisma-generate": {
      "executor": "@nx/run-commands:run-commands",
      "options": {
        "command": "prisma generate",
        "cwd": "apps/angular"
      }
    },
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/angular",
        "index": "apps/angular/src/index.html",
        "browser": "apps/angular/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/angular/tsconfig.app.json",
        "assets": [
          "apps/angular/src/assets",
          {
            "input": "apps/angular/public",
            "glob": "**/*",
            "output": "/public"
          }
        ],
        "styles": [
          "apps/angular/src/styles.css",
          "./node_modules/primeng/resources/themes/saga-blue/theme.css",
          "./node_modules/primeng/resources/primeng.min.css",
          "./node_modules/primeicons/primeicons.css",
          "./node_modules/primeflex/primeflex.css",
          "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
          "node_modules/sweetalert2/src/sweetalert2.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules/"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2MB",
              "maximumError": "4MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "sourceMap": false
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "angular:build:production"
        },
        "development": {
          "buildTarget": "angular:build:development"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "angular:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/angular/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "angular:build",
        "port": 4200,
        "staticFilePath": "dist/apps/angular/browser",
        "spa": true
      }
    }
  }
}
