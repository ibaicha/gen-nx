  "scripts": {
    "test:ci:angular": "nx test angular --ci --codeCoverage",
    "test:ci:api": "nx test api --ci --codeCoverage",
    "test:ci": "nx run-many --target=test --all --ci --parallel --maxParallel=3",
    "test:ci:all": "npm run test:ci:angular && npm run test:ci:nest",
    "seed": "dotenv -e apps/api/.env -- ts-node-dev apps/api/src/prisma/seed.ts",
    "seed-dev": "dotenv -e apps/api/.env -- ts-node-dev apps/api/src/prisma/seed.ts",
    "start:migrate:dev:init": "dotenv -e apps/api/.env -- npx prisma migrate dev --name init --schema=./apps/api/prisma/schema.prisma",
    "start:migrate:reset": "dotenv -e apps/api/.env -- npx prisma migrate reset --schema=./apps/api/prisma/schema.prisma",
    "start:migrate:prod": "npx prisma migrate deploy --schema=./apps/api/prisma/schema.prisma && npx nx run api:serve ",
    "start:prod": "npx nx run api:serve ",
    "studio": "dotenv -e apps/api/.env -- npx prisma studio --schema=./apps/api/prisma/schema.prisma",
    "format": "prettier --write \"apps/{api,angular}/src/**/*.ts\"",
    "docker:clean": "./docker-nx-clean.sh",
    "docker:restart": "./docker-nx-restart.sh",
    "docker:restart-prod": "./docker-nx-restart-prod.sh",
    "docker:restart:angular": "./docker-nx-restart-cible.sh angular-app",
